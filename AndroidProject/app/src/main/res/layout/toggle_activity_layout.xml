<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/toggle_activity_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.neelanjana.benchmarkalgo.Toggle_activity">

    <Button
        android:text="CR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btn_cc"
        android:layout_alignBottom="@+id/btn_cc"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/btn_cr"
        android:onClick="toogleMsg"/>

    <Button
        android:text="CL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:id="@+id/btn_cl"
        android:layout_below="@+id/btn_tl"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="toogleMsg"/>

    <Button
        android:text="TL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="61dp"
        android:id="@+id/btn_tl"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="toogleMsg"/>

    <Button
        android:text="TR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/btn_tc"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/btn_tr"
        android:onClick="toogleMsg"/>

    <Button
        android:text="CC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn_cc"
        android:layout_alignBottom="@+id/btn_cl"
        android:layout_alignLeft="@+id/btn_tc"
        android:layout_alignStart="@+id/btn_tc"
        android:layout_alignTop="@+id/btn_cl"
        android:onClick="toogleMsg"/>

    <Button
        android:text="TC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn_tc"
        android:layout_alignBaseline="@+id/btn_tl"
        android:layout_alignBottom="@+id/btn_tl"
        android:layout_centerHorizontal="true"
        android:onClick="toogleMsg"/>

    <Button
        android:text="BC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn_bc"
        android:layout_alignBaseline="@+id/btn_bl"
        android:layout_alignBottom="@+id/btn_bl"
        android:layout_alignLeft="@+id/btn_cc"
        android:layout_alignStart="@+id/btn_cc"
        android:onClick="toogleMsg"/>

    <Button
        android:text="BR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn_br"
        android:layout_alignBaseline="@+id/btn_bc"
        android:layout_alignBottom="@+id/btn_bc"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="toogleMsg"/>

    <Button
        android:text="BL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn_bl"
        android:layout_marginBottom="81dp"
        android:onClick="toogleMsg"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
